{
    "name": "calendar-website",
    "version": "1.0.0",
    "main": "dist/app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
        "watch-ts": "tsc -w",
        "start-backend": "nodemon ./dist/app.js",
        "start-frontend": "cd ../client && npm run start",
        "dev-back": "concurrently \"npm run prettier-watch\" \"npm run watch-ts\" \"npm run start-backend\"",
        "dev": "concurrently \"npm run prettier-watch\" \"npm run watch-ts\" \"npm run start-backend\" \"npm run start-frontend\""
    },
    "dependencies": {
        "body-parser": "^1.19.0",
