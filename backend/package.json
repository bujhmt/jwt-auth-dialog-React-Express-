    "devDependencies": {
        "@types/config": "0.0.36",
        "@types/express": "^4.16.1",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^14.11.1",
        "concurrently": "^5.3.0",
        "morgan": "^1.10.0",
        "typescript": "^4.1.3",
        "nodemon": "^2.0.4",
        "onchange": "^7.0.2",
        "prettier": "^2.1.2",
        "tslint": "^6.1.3"
    }
